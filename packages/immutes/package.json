{
  "private": true,
  "name": "immutes",
  "type": "module",
  "description": "immutable primitives and utils",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.esm.js"
    }
  },
  "scripts": {
    "build": "tsup src/main.ts --format cjs,esm --dts",
    "dev": "tsup src/main.ts --format cjs,esm --dts --watch",
    "test:unit": "vitest",
    "type-check": "tsc -b",
    "lint": "eslint .",
    "format": "prettier . --write",
    "check": "prettier --write . && eslint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "vite-plugin-dts": "^4.5.4",
    "tsup": "^6.5.0"
  }
}
